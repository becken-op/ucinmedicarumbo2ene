<?xml version="1.0" encoding="UTF-8"?>
<odoo>
    <record id="l10n_mx_efos_view_tree" model="ir.ui.view">
        <field name="name">l10n_mx.efos.view.form.tree</field>
        <field name="model">l10n_mx.efos</field>
        <field name="arch" type="xml">
            <tree>
                <field name="name"/>
                <field name="rfc"/>
                <field name="state"/>
            </tree>
        </field>
    </record>

    <record id="l10n_mx_efos_action" model="ir.actions.act_window">
        <field name="name">EFOS</field>
        <field name="res_model">l10n_mx.efos</field>
        <field name="view_mode">tree</field>
    </record>

    <menuitem id="l10n_mx_efos_menu_item" name="EFOS" parent="account.menu_finance_configuration" action="l10n_mx_efos_action" groups="base.group_no_one"/>
</odoo>
