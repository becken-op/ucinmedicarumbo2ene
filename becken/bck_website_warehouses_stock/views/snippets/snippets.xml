<?xml version="1.0" encoding="utf-8"?>
<odoo>
  <template id="snippets" inherit_id="website.snippets" name="Bck Warehouse list">
    <xpath expr="//div[@id='snippet_feature']/div[@class='o_panel_body']" position="inside">
      <t t-snippet="bck_website_warehouses_stock.s_warehouses"
        t-thumbnail="/bck_website_warehouses_stock/static/src/img/snippets_thumbs/warehouses_stock.svg" />
    </xpath>
  </template>

  <template id="assets_snippet_s_warehouses_js_000" inherit_id="website.assets_frontend">
    <xpath expr="//script[last()]" position="after">
        <script type="text/javascript" src="/bck_website_warehouses_stock/static/src/snippets/s_warehouses/000.js"/>
    </xpath>
  </template>

</odoo>
